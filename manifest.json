{
  "description": "Paper GTK theme for the Chrome browser",
  "manifest_version": 2,
  "name": "Paper GTK",
  "theme": {
    "colors": {
      "bookmark_text": [76, 76, 76],
      "button_background": [0, 0, 0],
      "frame": [84, 110, 122],
      "frame_inactive": [71, 87, 95],
      "frame_incognito": [84, 110, 122],
      "frame_incognito_inactive": [71, 87, 95],
      "ntp_background": [255, 255, 255],
      "ntp_text": [0, 0, 0],
      "tab_background_text": [64, 64, 64],
      "tab_text": [0, 0, 0],
      "toolbar": [223, 223, 223]
    },
    "images": {
      "theme_frame": "images/theme_frame.png",
      "theme_frame_inactive": "images/theme_frame_inactive.png",
      "theme_incongnito_frame": "images/theme_frame.png",
      "theme_frame_incognito_inactive": "images/theme_frame_inactive.png"
    },
    "properties": {
      "ntp_background_alignment": "bottom",
      "ntp_background_repeat": "no-repeat"
    },
    "tints": {
      "buttons": [0, 0, 0.5],
      "background_tab": [-1, 0.5, 0.85],
      "frame": [-1, -1, -1],
      "frame_inactive": [-1, -1, -1],
      "frame_incognito": [-1, -1, -1],
      "frame_incognito_inactive": [-1, -1, -1]
    }
  },
  "version": "1.3"
}
